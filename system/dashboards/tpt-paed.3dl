<TabSet>

  <Tab title="Never initiated TPT">
    <Dashboard>
      <Grid>
        <Row  columns = "1"   largeColumns = "1" >
                <DataContainer query={` SELECT SELECT "ART Number","Sex", "Current Age", CASE WHEN "Last Pregnancy Status"='Yes' THEN 'Pregnant' WHEN "Last Breast Feeding"='Yes' THEN 'Breast Feeding' ELSE NULL END AS "PBF Status",  to_char("ART Start Date" , 'DD Mon YYYY') as "ART Start", "Months Since ART Start" as "ART Duration (months)", "TPT Regimen", "TPT Start Date", "TPT Expected Stop Date", "TPT Actual Stop Date", "TPT Adherance", "TPT Status", "TPT Stop Reason" as "Stop Reason" FROM analysis.fact_sentinel_event se INNER JOIN analysis.dim_age_group ag ON se."Last Visit Age"=ag.age WHERE "TPT Status Two" = 'Never initiated TPT' AND "TX Curr" = 'Yes' AND  ag.paeds_adult_age_group='0-18 yr old (Pediatric)' LIMIT 100`}>
                    <SmartDataTable header="Never initiated TPT"/>
                </DataContainer>
            </Row>
      </Grid>
    </Dashboard>
  </Tab>

  <Tab title="Initiated but not completed">
    <Dashboard>
      <Grid>
        <Row  columns = "1"   largeColumns = "1">
            <DataContainer query={`  SELECT SELECT "ART Number","Sex", "Current Age", CASE WHEN "Last Pregnancy Status"='Yes' THEN 'Pregnant' WHEN "Last Breast Feeding"='Yes' THEN 'Breast Feeding' ELSE NULL END AS "PBF Status",  to_char("ART Start Date" , 'DD Mon YYYY') as "ART Start", "Months Since ART Start" as "ART Duration (months)", "TPT Regimen", "TPT Start Date", "TPT Expected Stop Date", "TPT Actual Stop Date", "TPT Adherance", "TPT Status", "TPT Stop Reason" as "Stop Reason" FROM analysis.fact_sentinel_event se INNER JOIN analysis.dim_age_group ag ON se."Last Visit Age"=ag.age WHERE "TPT Status Two" = 'Have not completed TPT' AND "TX Curr" = 'Yes' AND  ag.paeds_adult_age_group='0-18 yr old (Pediatric)'  LIMIT 100`}>
                <SmartDataTable header="Initiated but not completed" />
            </DataContainer>
        </Row>
      </Grid>
    </Dashboard>
  </Tab>

</TabSet>